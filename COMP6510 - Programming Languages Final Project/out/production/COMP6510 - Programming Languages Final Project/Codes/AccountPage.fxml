<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #353535;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Codes.AccountPageController">
   <children>
      <AnchorPane layoutX="22.0" layoutY="23.0" prefHeight="354.0" prefWidth="557.0" style="-fx-background-color: FF9100;">
         <children>
            <Label alignment="CENTER" layoutX="171.0" layoutY="21.0" prefHeight="36.0" prefWidth="215.0" text="Account Settings" textAlignment="CENTER" textFill="WHITE">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
            <GridPane layoutX="79.0" layoutY="79.0" prefHeight="196.0" prefWidth="400.0">
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="120.0" minWidth="0.0" prefWidth="110.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="295.0" minWidth="0.0" prefWidth="290.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <Label prefHeight="30.0" prefWidth="110.0" text="Username :" textFill="WHITE">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <Label prefHeight="30.0" prefWidth="110.0" text=" Income :" textFill="WHITE" GridPane.rowIndex="4">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <Label prefHeight="33.0" prefWidth="110.0" text="Password :" textFill="WHITE" GridPane.rowIndex="2">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <TextField fx:id="usernameTextField" promptText="ex. MinnySky123, GoldenEGz66, etc" GridPane.columnIndex="1" />
                  <TextField fx:id="passwordTextField" promptText="password" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <TextField fx:id="incomeTextField" promptText="ex. Rp 5000000" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <Label fx:id="UsernameWarningLabel" alignment="CENTER" prefHeight="17.0" prefWidth="290.0" textAlignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label fx:id="PasswordWarningLabel" alignment="CENTER" prefHeight="17.0" prefWidth="290.0" textAlignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <Label fx:id="IncomeWarningLabel" alignment="CENTER" prefHeight="17.0" prefWidth="290.0" textAlignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="5" />
               </children>
            </GridPane>
            <Button fx:id="EditItemButton" layoutX="287.0" layoutY="281.0" mnemonicParsing="false" onAction="#ConfirmButtonClicked" prefHeight="36.0" prefWidth="136.0" text="Confirm">
               <font>
                  <Font size="13.0" />
               </font>
            </Button>
            <Button layoutX="136.0" layoutY="281.0" mnemonicParsing="false" onAction="#CancelButtonClicked" prefHeight="36.0" prefWidth="136.0" text="Cancel">
               <font>
                  <Font size="13.0" />
               </font>
            </Button>
